                            <Grid>
                                <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Glow Halfcircle.png" Stretch="Fill" Margin="800,486,-136,175" RenderTransformOrigin="0.5,0.5" Opacity="0.8">
                                    <Image.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform/>
                                            <SkewTransform/>
                                            <RotateTransform Angle="-1"/>
                                            <TranslateTransform/>
                                        </TransformGroup>
                                    </Image.RenderTransform>
                                </Image>
                                <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Glow Beam.png" Stretch="Fill" RenderTransformOrigin="0.5,0.5" Margin="1095,427,206,140" Opacity=".48">
                                    <Image.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform/>
                                            <SkewTransform/>
                                            <RotateTransform Angle="185"/>
                                            <TranslateTransform/>
                                        </TransformGroup>
                                    </Image.RenderTransform>
                                </Image>
                                <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Glow Beam.png" Stretch="Fill" RenderTransformOrigin="0.5,0.5" Margin="939,467,354,107" Opacity=".74">
                                    <Image.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform/>
                                            <SkewTransform/>
                                            <RotateTransform Angle="152"/>
                                            <TranslateTransform/>
                                        </TransformGroup>
                                    </Image.RenderTransform>
                                </Image>
                            </Grid>
                            <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Cloud 4.png" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="1360" Height="85" Margin="0,0,0,122"/>
                            <Grid>
                                <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Glow Halfcircle.png" Stretch="Fill" Margin="952,512,-244,129" RenderTransformOrigin="0.5,0.5" Opacity="0.8">
                                    <Image.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform/>
                                            <SkewTransform/>
                                            <RotateTransform Angle="-2"/>
                                            <TranslateTransform/>
                                        </TransformGroup>
                                    </Image.RenderTransform>
                                </Image>
                                <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Glow Beam.png" Stretch="Fill" RenderTransformOrigin="0.5,0.5" Margin="1124,490,168,52" Opacity=".56">
                                    <Image.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform/>
                                            <SkewTransform/>
                                            <RotateTransform Angle="191"/>
                                            <TranslateTransform/>
                                        </TransformGroup>
                                    </Image.RenderTransform>
                                </Image>
                            </Grid>
                            <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Cloud 2.png" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="1360" Height="128" Margin="0,0,0,21"/>
                        </Grid>
                        <Grid x:Name="BottomHighClouds">
                            <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Mini Cloud 1.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="-70,244,0,0" Height="102"/>
                            <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Mini Cloud 2.png" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,174,-252,0" Height="121"/>
                        </Grid>
                        <Grid x:Name="TopHighClouds">
                            <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Mini Cloud 3.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="-92,-50,0,0" Height="223"/>
                            <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Mini Cloud 4.png" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,-76,-52,0" Height="223"/>
                        </Grid>
                        <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Glow Halfcircle.png" Stretch="Fill" Margin="823,567,45,-2" RenderTransformOrigin="0.5,0.5">
                            <Image.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="-6"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Image.RenderTransform>
                        </Image>
                        <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Glow Halfcircle.png" Stretch="Fill" Margin="79,524,747,0"/>
                        <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Glow Beam.png" Stretch="Fill" RenderTransformOrigin="0.5,0.5" Margin="1060,517,214,-117" Opacity=".84">
                            <Image.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="158"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Image.RenderTransform>
                        </Image>
                        <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Glow Halfcircle.png" Stretch="Fill" Margin="-90,496,958,69" RenderTransformOrigin="0.5,0.5">
                            <Image.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="12"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Image.RenderTransform>
                        </Image>
                        <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Cloud 1.png" VerticalAlignment="Bottom" HorizontalAlignment="Center"/>
                    </Grid>
                </Grid>
            </Grid>
            <Grid x:Name="BlackNWhiteThemeGrid" Visibility="Collapsed">
                <Grid.Background>
                    <ImageBrush ImageSource="/Assets/Backgrounds/BlackNWhite/Bgr.png" Stretch="Fill"/>
                </Grid.Background>
            </Grid>
        </Grid>
        <Grid x:Name="AlwaysBehind">
            <Grid x:Name="ScreenHeader">
                <Image CacheMode="BitmapCache" Source="/Assets/Header.png" Stretch="Fill" VerticalAlignment="Top" HorizontalAlignment="Center" Height="34"/>
            </Grid>
        </Grid>
        <Grid x:Name="UserInterfaces">
            <Grid x:Name="MainMenuUI" Visibility="Hidden">
                <Grid Visibility="Hidden" x:Name="MoneyUI" VerticalAlignment="Top" HorizontalAlignment="Right" Height="55" Width="170" Margin="0,10,143,0" MouseLeftButtonUp="MoneyUI_MouseLeftButtonUp">
                    <Image Source="/Assets/Left Notch Header.png" HorizontalAlignment="Left"/>
                    <Image Source="/Assets/Right Notch Header.png" HorizontalAlignment="Right"/>
                    <Image Source="/Assets/Header.png" HorizontalAlignment="Center" Width="157" Stretch="Fill"/>
                    <Image Source="Assets/Icons/Gem.png" HorizontalAlignment="Left" Margin="-21,0,0,-2" Width="53"/>
                    <ExtraThings:DrawTextOutlined x:Name="MoneyCount" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" FontSize="38" Text="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Margin="32,0,4,0"/>
                </Grid>
                <Grid x:Name="LogosCorner" HorizontalAlignment="Left" VerticalAlignment="Top" Width="400" Height="170" Margin="10,45,0,0" CacheMode="BitmapCache">
                    <ExtraThings:GridButton x:Name="BTN_LogoToggler" MouseLeftButtonUp="BTN_LogoToggler_MouseLeftButtonUp">
                        <!-- To make the entire button pressable instead of only the rendered elements be pressable -->
                        <Image Source="Assets/Dummy.png" Stretch="Fill"/>
                        <Grid x:Name="LogosCorner_OldStyle" Visibility="Visible">
                            <Image Source="/Assets/BS Logo Old.png" HorizontalAlignment="Left" VerticalAlignment="Top" Width="187"/>
                            <ExtraThings:DrawTextOutlined Text="{Binding Path=[TID_EVENT_TRACKER_SUBTEXT], Source={x:Static Lib:LocalizationLib.Strings}}" StrokeThiccness="3" ShadowColor="Black" ShadowOffset="0, 1.75" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" Margin="192,0,0,0" Height="150" Width="208"/>
                        </Grid>
                        <Grid x:Name="LogosCorner_NewStyle" Visibility="Collapsed">
                            <Image Source="/Assets/BS Logo New.png" HorizontalAlignment="Center" VerticalAlignment="Top" Width="271" Height="150"/>
                            <ExtraThings:DrawTextOutlined Text="{Binding Path=[TID_EVENT_TRACKER_SUBTEXT], Source={x:Static Lib:LocalizationLib.Strings}}" StrokeThiccness="3" ShadowColor="Black" ShadowOffset="0, 1.75" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" HorizontalAlignment="Center" VerticalAlignment="Top" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" Margin="0,130,0,0" Width="400" Height="40"/>
                        </Grid>
                    </ExtraThings:GridButton>
                </Grid>
                <Grid x:Name="SimpleView">
                    <Grid x:Name="DynCounter" VerticalAlignment="Center" HorizontalAlignment="Center" Width="1350" Height="220">
                        <Grid VerticalAlignment="Top" Height="148">
                            <Grid x:Name="DynCounter_ImgGrid" Width="675" HorizontalAlignment="Left">
                                <Image CacheMode="BitmapCache" x:Name="DynCounter_Img" Source="/Assets/Icons/Mastery Point.png" Height="146" Width="124" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,0,0"/>
                            </Grid>
                            <Grid x:Name="DynCounter_NumGrid" Width="675" HorizontalAlignment="Right">
                                <ExtraThings:DrawTextOutlined CacheMode="BitmapCache" x:Name="DynCounter_Num_CanMono" Text="0" FontSize="128" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" StrokeThiccness="8" ShadowColor="Black" ShadowOffset="0, 4.5" Margin="8, 0, 0, 0"/>
                            </Grid>
                        </Grid>
                        <Grid VerticalAlignment="Top" Height="52" Margin="0, 148, 0, 0">
                            <ExtraThings:DrawTextOutlined x:Name="Txt_NextMilestone" Text="{Binding Path=[TID_POINTS_TO_NEXT_MILESTONE], Source={x:Static Lib:LocalizationLib.Strings}}" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" FontSize="32" StrokeThiccness="2.5" ShadowColor="Black" ShadowOffset="0, 1.85" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" Width="1260"/>
                        </Grid>
                    </Grid>
                    <Grid x:Name="SystemClock" HorizontalAlignment="Right" VerticalAlignment="Top" Width="300" Height="55" Margin="0,85,10,0">
                        <ExtraThings:DrawTextOutlined x:Name="Txt_SystemClock" Text="Date goes here Abg&#10;Time goes here Abg" FontSize="22" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" HorizontalContentAlignment="Right" VerticalContentAlignment="Top" StrokeThiccness="1.75" ShadowColor="Black" ShadowOffset="0, 1.35"/>
                    </Grid>
                    <Grid x:Name="StatusCheck" VerticalAlignment="Top" HorizontalAlignment="Center" Height="52" Margin="0,85,0,0" Width="220">
                        <ExtraThings:DrawTextOutlined x:Name="Txt_Status" Text="" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" FontSize="32" StrokeThiccness="2.5" ShadowColor="Black" ShadowOffset="0, 1.85" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" Width="1260"/>
                    </Grid>
                </Grid>
                <Grid x:Name="DetailedView">
                    <Grid x:Name="ProgressBar" HorizontalAlignment="Center" VerticalAlignment="Bottom" Height="80" Margin="0,0,0,180" Width="1280">

                        <Grid x:Name="ProgressBar_Back">
                            <Image Source="/Assets/Bars/Progress Bar Back Left.png" HorizontalAlignment="Left" Margin="1, 2, 0, 3" CacheMode="BitmapCache"/>
                            <Image Source="/Assets/Bars/Progress Bar Back Right.png" HorizontalAlignment="Right" Margin="1, 2, 2, 3" CacheMode="BitmapCache"/>
                            <Image Source="/Assets/Bars/Progress Bar Back Middle.png" HorizontalAlignment="Center" Margin="1, 2, 0, 3" Width="1261" Stretch="Fill" CacheMode="BitmapCache"/>
                        </Grid>

                        <Grid x:Name="ProgressBar_Front" Visibility="Visible">
                            <Image x:Name="ProgressBar_FLeft" Visibility="Hidden" Source="/Assets/Bars/Progress Bar Fore Left.png" HorizontalAlignment="Left" Margin="1, 2, 0, 3" Width="11" CacheMode="BitmapCache"/>
                            <Image x:Name="ProgressBar_FRight" Visibility="Hidden" Source="/Assets/Bars/Progress Bar Fore Right.png" HorizontalAlignment="Left" Margin="8, 2, 0, 3" Width="11" CacheMode="BitmapCache"/>
                            <Image x:Name="ProgressBar_FMid" Visibility="Hidden" Source="/Assets/Bars/Progress Bar Fore Middle.png" HorizontalAlignment="Left" Margin="8, 2, 0, 3" Width="0" Stretch="Fill" CacheMode="BitmapCache"/>
                            <Image x:Name="ProgressBar_FProc" Visibility="Hidden" Source="/Assets/Bars/Progress Bar Proc.png" HorizontalAlignment="Left" Margin="3, 3.5, 0, 4" Width="16" Stretch="Fill"/>
                        </Grid>

                        <Grid x:Name="ProgressBar_Stencil">
                            <Image CacheMode="BitmapCache" Source="/Assets/Bars/Loading Bar Stencil Left.png" HorizontalAlignment="Left"/>
                            <Image CacheMode="BitmapCache" Source="/Assets/Bars/Loading Bar Stencil Right.png" HorizontalAlignment="Right"/>
                            <Image CacheMode="BitmapCache" Source="/Assets/Bars/Loading Bar Stencil Middle.png" HorizontalAlignment="Center" Stretch="Fill" Width="1253.5"/>
                        </Grid>

                        <ExtraThings:DrawTextOutlined CacheMode="BitmapCache" x:Name="Txt_ProgressPercent" Text="0%" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" FontSize="64" StrokeThiccness="4.5" ShadowColor="Black" ShadowOffset="0, 3.65" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="1260"/>
                    </Grid>
                </Grid>
                <Grid x:Name="TooMuchStuffView">
