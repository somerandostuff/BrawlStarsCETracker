                                </Image>
                            </Grid>
                            <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/AvD_Angels/Cloud 3.png" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="1360" Height="108" Margin="0,0,0,68"/>
