<Window x:Class="EventTrackerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EventTrackerWPF"
        xmlns:ExtraThings="clr-namespace:EventTrackerWPF.CustomElements"
        xmlns:Lib="clr-namespace:EventTrackerWPF.Librarbies"
        mc:Ignorable="d"
        Title="hello" Height="720" Width="1360" ResizeMode="CanMinimize" Closing="Window_Closing" Loaded="Window_Loaded" Icon="/Assets/BS Star Wings.ico">
    <Grid x:Name="MainGrid">
        <Grid>
            <Grid.Background>
                <!-- For easier viewing without interfering with the cooler background -->
                <SolidColorBrush>Gray</SolidColorBrush>
            </Grid.Background>
        </Grid>
        <Grid x:Name="ThemesGallery" IsHitTestVisible="False">
            <Grid x:Name="BasicThemeGrid" Visibility="Collapsed">
                <Grid.Background>
                    <ImageBrush ImageSource="/Assets/Backgrounds/Basic/Bgr.png" Stretch="Fill"/>
                </Grid.Background>
                <Grid Name="Grid_Bg_ForeItems_Basic">
                    <Image x:Name="BasicIconsImage"
               Source="/Assets/Backgrounds/Basic/Icons.png"
               Stretch="Uniform"
               RenderTransformOrigin="0.5,0.5"
               Opacity=".1"
               CacheMode="BitmapCache"
               Margin="-562,-128,-14,-476">
                        <Image.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform Angle="8"/>
                                <TranslateTransform x:Name="BasicImageTranslate"/>
                            </TransformGroup>
                        </Image.RenderTransform>
                    </Image>
                </Grid>
            </Grid>
            <Grid x:Name="BrawloweenThemeGrid" Visibility="Collapsed">
                <Grid.Background>
                    <ImageBrush ImageSource="/Assets/Backgrounds/Brawloween2023/Bgr.png" Stretch="Fill"/>
                </Grid.Background>
                <Grid Name="Grid_Bg_ForeItems_Brawloween">
                    <Image x:Name="GhostIconsImage"
               Source="/Assets/Backgrounds/Brawloween2023/Ghost Icons.png"
               Stretch="Uniform"
               RenderTransformOrigin="0.5,0.5"
               Opacity=".07"
               CacheMode="BitmapCache"
               Margin="-562,-128,-14,-476">
                        <Image.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform Angle="8"/>
                                <TranslateTransform x:Name="GhostImageTranslate"/>
                            </TransformGroup>
                        </Image.RenderTransform>
                    </Image>
                    <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/Brawloween2023/Ground.png" Margin="0,472,0,0" Stretch="Fill"/>
                    <Image CacheMode="BitmapCache" Source="/Assets/Backgrounds/Brawloween2023/Blur.png" Margin="0,565,0,0" HorizontalAlignment="Center" VerticalAlignment="Top" Stretch="Fill" Height="207"/>
