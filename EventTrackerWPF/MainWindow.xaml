                        <ExtraThings:DrawTextOutlined x:Name="Txt_Approximation" Text="Speed: 0/s" Fill="Orange" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" FontSize="32" StrokeThiccness="2.5" ShadowColor="Black" ShadowOffset="0, 1.85" HorizontalAlignment="Left" HorizontalContentAlignment="Left" VerticalContentAlignment="Top" Width="420"/>
                        <ExtraThings:DrawTextOutlined x:Name="Txt_ETA" Text="ETA: N/A" Fill="Lime" FontFamily="pack://application:,,,/EventTrackerWPF;component/Assets/LilitaOne-Regular.ttf#Lilita One" FontSize="32" StrokeThiccness="2.5" ShadowColor="Black" ShadowOffset="0, 1.85" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" Width="420"/>
                    </Grid>
                </Grid>
